{
  "manifest_version": 1,
  "name": "Fluxboard",
  "version": "1.0",
  "description": "A personalized new tab page with bookmarks, todos, and quick notes",
  
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  
  "permissions": [
    "storage"
  ],
  
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}