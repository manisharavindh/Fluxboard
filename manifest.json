{
  "manifest_version": 2,
  "name": "Fluxboard",
  "version": "1.0",
  "description": "A personalized new tab page with bookmarks, todos, and quick notes",
  
  "chrome_url_overrides": {
    "newtab": "src/index.html"
  },

  "chrome_settings_overrides": {
    "homepage": "src/index.html"
  },
  
  "permissions": [
    "tabs",
    "storage",
    "bookmarks",
    "activeTab",
    "contextMenus"
  ],
  
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}